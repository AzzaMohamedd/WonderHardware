
@{
    ViewData["Title"] = "Users";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="content" style="background: #f1f2f7;">
    <div class="breadcrumbs">
        <div class="breadcrumbs-inner">
            <div class="row m-0">
                <div class="col-sm-4">
                    <div class="page-header float-left">
                        <div class="page-title">
                            <h1>Dashboard</h1>
                        </div>
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="page-header float-right">
                        <div class="page-title">
                            <ol class="breadcrumb text-right">
                                <li><a href="/Admin/Admin/Index">Dashboard</a></li>
                                <li class="active"> Case </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="content">
        <div class="animated fadeIn">
            <div class="row">

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header" style=" display: flex; justify-content: space-between;">
                                <strong class="card-title">Cases Table</strong>
                                <button type="button" class="btn btn-outline-info">+</button>
                        </div>
                        <div class="card-body">
                            <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                <thead>
                                    <tr style=" text-align: center;">
                                        <th style=" vertical-align: middle;">Name</th>
                                        <th style=" vertical-align: middle;">Code</th>
                                        <th style=" vertical-align: middle;">Brand</th>
                                        <th style=" vertical-align: middle;">Price</th>
                                        <th style=" vertical-align: middle;">Quantity</th>
                                        <th style=" vertical-align: middle;">Factory Size</th>
                                        <th style=" vertical-align: middle;">Rate</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>Case Name</td>
                                        <td>C2</td>
                                        <td>intel</td>
                                        <td>1765 LE</td>
                                        <td>30</td>
                                        <td>Mini Tower</td>
                                        <td>60%</td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>


            </div>
        </div><!-- .animated -->
    </div><!-- .content -->
</div>

