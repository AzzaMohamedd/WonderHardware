                                            <h3 class="product-name"><a href="#">@item.MotherName</a></h3>
                                            <h4 class="product-price">@item.MotherPrice <span>LE</span></h4>
                                            <div class="product-rating">
                                                @for (var i = 1; i < Decimal.Round(item.MotherRate, 1); i++)
                                                {
                                                    <i class="fa fa-star"></i>
                                                }
                                                @for (var i = Decimal.Round(item.MotherRate, 1); i <= 5; i++)
                                                {
                                                    @if (Decimal.Round(item.MotherRate, 1) != 0)
                                                    {
                                                        @if (Math.Floor((i - Math.Floor(i)) * 10) == 0 && i == Decimal.Round(item.MotherRate, 1))
                                                        {
                                                            <i class="fa fa-star"></i>
                                                        }
                                                        else if (Math.Floor((i - Math.Floor(i)) * 10) >= 5 && i == Decimal.Round(item.MotherRate, 1))
                                                        {
                                                            <i class="fa fa-star-half-o" style="color: #D10024"></i>
                                                        }
                                                        else
                                                        {
                                                            <i class="fa fa-star-o" style="color: #D10024"></i>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        if (i < 5)
                                                        {
                                                            <i class="fa fa-star-o" style="color: #D10024"></i>
                                                        }
                                                    }
                                                }
                                            </div>
                                            <div class="product-btns">
                                                @if(item.WishList == true){
                                                    <button onclick="AddOrDeleteWL('@item.MotherCode' ,'Delete')" class="add-to-wishlist"><i style="color: #D10024" class="fa fa-heart"></i><span class="tooltipp">Remove from wishlist</span></button>
                                                }
                                                else
