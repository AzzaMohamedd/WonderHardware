                                @foreach (var item in NewMotherBoards)
                                {
                                    <div class="product">
                                        <div class="product-img">
                                            <img src="~/img/product01.png" alt="">
                                            <div class="product-label">
                                                <span class="new">NEW</span>
                                            </div>
                                        </div>
                                        <div class="product-body">
                                            <h3 class="product-name"><a href="#">@item.MotherName</a></h3>
                                            <h4 class="product-price">@item.MotherPrice <span>LE</span></h4>
                                            <div class="product-rating">
                                                @for (var i = 1; i < Decimal.Round(item.MotherRate, 1); i++)
                                                {
                                                    <i class="fa fa-star"></i>
                                                }
                                                @for (var i = Decimal.Round(item.MotherRate, 1); i <= 5; i++)
                                                {
                                                    @if (Decimal.Round(item.MotherRate, 1) != 0)
                                                    {
                                                        @if (Math.Floor((i - Math.Floor(i)) * 10) == 0 && i == Decimal.Round(item.MotherRate, 1))
                                                        {
                                                            <i class="fa fa-star"></i>
                                                        }
                                                        else if (Math.Floor((i - Math.Floor(i)) * 10) >= 5 && i == Decimal.Round(item.MotherRate, 1))
                                                        {
                                                            <i class="fa fa-star-half-o" style="color: #D10024"></i>
                                                        }
                                                        else
                                                        {
                                                            <i class="fa fa-star-o" style="color: #D10024"></i>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        if (i < 5)
                                                        {
                                                            <i class="fa fa-star-o" style="color: #D10024"></i>
                                                        }
                                                    }
                                                }
                                            </div>
                                            <div class="product-btns">
                                                <button class="add-to-wishlist"><i class="fa fa-heart-o"></i><span class="tooltipp">add to wishlist</span></button>
                                                <button onclick="gotoDetails('@item.MotherCode')" class="quick-view"><i class="fa fa-eye"></i><span class="tooltipp">quick view</span></button>
                                                <button onclick="AddToCart({ Image: '@item.Image', Name: '@item.MotherName', Code: '@item.MotherCode', Price: @item.MotherPrice ,Quantity: 1})" data-toggle="blog-tags" data-placement="top" title="Add TO CART">
